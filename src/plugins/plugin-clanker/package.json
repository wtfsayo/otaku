{
  "name": "@elizaos/plugin-clanker",
  "description": "Clanker protocol integration plugin for ElizaOS - enables token deployment, management, and trading on Base L2",
  "version": "1.2.12",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "author": "ElizaOS Team",
  "keywords": [
    "plugin",
    "elizaos",
    "clanker",
    "base",
    "defi",
    "tokens",
    "trading"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/core": "latest",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "8.5.0",
    "typescript": "5.8.2",
    "prettier": "3.5.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "lint": "prettier --write ./src",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.24.2"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "PRIVATE_KEY": {
        "type": "string",
        "description": "Private key for wallet transactions (use with caution)",
        "required": true
      },
      "BASE_RPC_URL": {
        "type": "string",
        "description": "Base L2 RPC endpoint URL",
        "required": true
      }
    }
  }
}
